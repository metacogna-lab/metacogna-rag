{
  "name": "pratejra-rag",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "packageManager": "bun@1.3.2",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "jest",
    "worker:dev": "bun wrangler dev",
    "worker:deploy": "bun wrangler deploy",
    "worker:tail": "bun wrangler tail",
    "db:create": "bun wrangler d1 create pratejra-db",
    "db:init": "bun wrangler d1 execute pratejra-db --file=db/schema.sql",
    "db:query": "bun wrangler d1 execute pratejra-db --command",
    "vector:create": "bun wrangler vectorize create pratejra-index --dimensions=768 --metric=cosine",
    "vector:info": "bun wrangler vectorize get pratejra-index",
    "deploy:setup": "bun run db:create && bun run vector:create && bun run db:init",
    "deploy:full": "bun run build && bun run worker:deploy"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "chromadb": "^1.8.1",
    "d3-scale": "^4.0.2",
    "framer-motion": "^11.0.0",
    "lucide-react": "^0.344.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "langfuse": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "@types/bun": "latest",
    "wrangler": "^3.95.0"
  },
  "module": "index.tsx"
}
